<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">Monthly</h4>
</div>

<table id="monthly" class="table table-condensed">
			<tr>
				<th class="col-sm-1">Month:</th>
				<th class="col-sm-1">rx</th>
				<th class="col-sm-1">tx:</th>
				<th class="col-sm-1">total:</th>
			</tr>
			<% @top.each do |x|%>
			<tr>
				<td class="active"><%= x[:date]%>
		  		<td class="active"><%= x[:rx]%>
			    <td class="active"><%= x[:tx]%>
			    <td class="active"><%= x[:total]%>
			</tr>
			<%end%>	
</table>